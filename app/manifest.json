{
    "manifest_version": 2,
    "name": "Stock Trace",
    "version": "1.0",
    "description": "查询股票",
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "icons": {
        "16": "images/icon16.png"
    },
    "omnibox": {
        "keyword": "stock"
    },
    "permissions": [
        "*://finance.sina.com.cn/*",
        "*://vip.stock.finance.sina.com.cn/*"
    ]
}