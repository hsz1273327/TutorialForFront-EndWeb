{
    "manifest_version": 3,
    "name": "Stock Trace",
    "version": "1.0",
    "description": "查询股票",
    "background": {
        "service_worker": "js/background.js",
        "type": "module"
    },
    "icons": {
        "16": "images/icon16.png"
    },
    "omnibox": {
        "keyword": "stock"
    },
    "permissions": [
        "*://finance.sina.com.cn/*",
        "*://vip.stock.finance.sina.com.cn/*"
    ]
}