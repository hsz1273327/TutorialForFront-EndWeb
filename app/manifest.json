{
    "manifest_version": 3,
    "name": "Cannottouch",
    "version": "1.0",
    "description": "永远点不到百度的搜索按钮",
    "icons": {
        "16": "images/icon16.png",
        "128": "images/icon128.png"
    },
    "action": {
        "default_icon": {
            "16": "images/icon16.png"
        }
    },
    "content_scripts": [
        {
            "matches": ["*://www.baidu.com/"],
            "js": ["js/content.js"]
        }
    ]
}